{
  "manifest_version": 2,
  "name": "__MSG_pluginTitle__",
  "version": "1.3.0",
  "description": "__MSG_pluginDesc__",
  "default_locale": "en",
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "icons/19.png",
      "38": "icons/38.png"
    },
    "default_title": "__MSG_pluginTitle__",
    "default_popup": "html/popup.html"
  },
  "background": {
    "scripts": ["js/jquery-3.2.1.min.js","js/background.js"],
    "persistent": false
  },
  "content_scripts": [{
    "run_at": "document_start",
    "js": ["js/restriction.js"],
    "matches": [
      "*://pc-play.games.dmm.co.jp/play/imys_r/*"
    ]
  }],
  "devtools_page": "html/devtools.html",
  "permissions": [
    "https://r.kamihimeproject.net/"
  ]
}
