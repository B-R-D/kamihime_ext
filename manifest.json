{
  "manifest_version": 2,
  "name": "神姫助手",
  "version": "1.1.3",
  "description": "网页游戏“神姬计划”的辅助工具。",
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "icons/19.png",
      "38": "icons/38.png"
    },
    "default_title": "神姫助手",
    "default_popup": "html/popup.html"
  },
  "background": {
    "scripts": ["js/jquery-3.2.1.min.js","js/background.js"],
    "persistent": false
  },
  "devtools_page": "html/devtools.html",
  "permissions": [
    "https://r.kamihimeproject.net/"
  ]
}
